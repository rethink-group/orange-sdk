{
    "name": "rethink-group/orange-sdk",
    "description": "SDK for building applications that integrate with Orange Management Systems.",
    "keywords": ["orange", "rethink-group", "sdk"],
    "license": "Proprietary",
    "authors": [
        {
            "name": "Jamie Howard",
            "email": "jhoward@rethinkgroup.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "RethinkGroup\\SDK\\": "src/SDK/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
