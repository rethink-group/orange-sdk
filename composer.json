{
    "name": "rethink-group/orange-sdk",
    "description": "SDK for building applications that integrate with Orange's systems.",
    "keywords": ["orange", "rethink-group", "sdk"],
    "license": "Proprietary",
    "authors": [
        {
            "name": "Jamie Howard",
            "email": "jhoward@rethinkgroup.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/contracts": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4",
        "league/oauth2-client": "~2.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "RethinkGroup\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
